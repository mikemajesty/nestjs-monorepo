    "url": "https://github.com/fastify/secure-json-parse/issues"
  },
  "homepage": "https://github.com/fastify/secure-json-parse#readme",
  "devDependencies": {
    "airtap": "^4.0.1",
    "airtap-playwright": "^1.0.1",
    "nyc": "^14.1.1",
    "playwright": "^1.7.1",
    "standard": "^17.0.0",
    "tape": "^5.1.1",
    "tsd": "^0.25.0"
  }
