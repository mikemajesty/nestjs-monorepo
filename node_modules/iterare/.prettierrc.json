{
  "tabWidth": 2,
  "printWidth": 120,
  "semi": false,
  "trailingComma": "es5",
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": "{tsconfig.json,.vscode/**/*.json}",
      "options": {
        "parser": "json5",
        "singleQuote": false,
        "quoteProps": "preserve"
      }
    }
  ]
}
